<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<!--命名空间应该是对应接口的包名+类名 -->
<mapper namespace="com.aiw.mapper.Common">
	
	
	<!--表名 -->
	<sql id="createEntityColumn">
		,createTime,lastUpdateTime,createUser,lastUpdateUser
	</sql>
	
	
	<sql id="createEntityColumnValue">
	,current_timestamp(),current_timestamp(),#{createUser},#{lastUpdateUser}
	</sql>
	
	

    
</mapper>